#include <iostream>
using namespace std;

int countDistinct(int arr[], int n) {
    int count = 0;

    for (int i = 0; i < n; i++) {
        bool isNew = true;

        // Check if this element has appeared before
        for (int j = 0; j < i; j++) {
            if (arr[i] == arr[j]) {
                isNew = false;
                break;
            }
        }

        if (isNew)
            count++;
    }

    return count;
}

int main() {
    int n;
    cout << "Enter the size of the array: ";
    cin >> n;

    int arr[n];
    cout << "Enter the array elements:\n";
    for (int i = 0; i < n; i++)
        cin >> arr[i];

    int totalUnique = countDistinct(arr, n);

    cout << "Total number of distinct elements: " << totalUnique << endl;

    return 0;
}
