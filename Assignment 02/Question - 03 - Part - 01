#include <iostream>
#include <vector>
using namespace std;

int findMissingNumber(const vector<int>& arr) 
{
    int n = arr.size() + 1;  
    for (int i = 0; i < arr.size(); ++i) 
    {
        if (arr[i] != i + 1)
        {
            return i + 1;
        }
    }
    return n;  
}

int main() 
{
    vector<int> arr = {1,2,3,5,6};  
    int missing = findMissingNumber(arr);
    cout << "Missing number: " << missing << endl;
    return 0;
}
