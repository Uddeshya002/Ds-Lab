#include <iostream>
using namespace std;
int main()
{
    char ch;
    cout << " Enter an upper character : ";
    cin >> ch;
    if(ch >= 'A' && ch <= 'Z')
    {
        char lower = ch + ('a' - 'A');
        cout << " Enter a lower character : " << lower << endl;
    }
    else
    {
        cout << " The chararacter is not uppercase : " << endl;
    }
    return 0;
}
