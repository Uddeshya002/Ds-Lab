#include <iostream>
#include <string>
using namespace std;
int main()
{
    string input,result = "";
    cout << " Enter the string : ";
    getline(cin,input);
    for(char ch : input)
    {
    if(!(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u' || ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U'))
    {
        result += ch;
    }
    }
    cout << " String without vowels : " << result << endl;
    return 0;
    
}
