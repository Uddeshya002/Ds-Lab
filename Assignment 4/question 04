#include <iostream>
#include <queue>
using namespace std;

void firstNonRepeating(string str) {
    queue<char> q;
    int freq[256] = {0};  // ASCII character frequency

    for (int i = 0; i < str.length(); i++) {
        char ch = str[i];

        // Ignore spaces
        if (ch == ' ')
            continue;

        freq[ch]++;
        q.push(ch);

        // Remove repeated chars from the front of the queue
        while (!q.empty() && freq[q.front()] > 1) {
            q.pop();
        }

        if (q.empty())
            cout << "-1 ";
        else
            cout << q.front() << " ";
    }
}

int main() {
    string input;
    cout << "Enter string: ";
    getline(cin, input);

    cout << "First non-repeating characters: ";
    firstNonRepeating(input);
    cout << endl;

    return 0;
}
