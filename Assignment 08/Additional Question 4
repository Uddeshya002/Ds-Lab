#include <iostream>
#include <queue>
#include <vector>
using namespace std;

struct Node{
    int data;
    Node *left,*right;
    Node(int v):data(v),left(NULL),right(NULL){}
};

Node* build(vector<int>& a){
    if(a[0]==-1) return NULL;
    Node* r=new Node(a[0]);
    queue<Node*> q;
    q.push(r);
    int i=1;
    while(!q.empty() && i<a.size()){
        Node* c=q.front(); q.pop();
        if(a[i]!=-1){ c->left=new Node(a[i]); q.push(c->left); }
        i++;
        if(i<a.size() && a[i]!=-1){ c->right=new Node(a[i]); q.push(c->right); }
        i++;
    }
    return r;
}

vector<int> rightView(Node* r){
    vector<int> v;
    if(!r) return v;
    queue<Node*> q;
    q.push(r);
    while(!q.empty()){
        int n=q.size();
        for(int i=0;i<n;i++){
            Node* c=q.front(); q.pop();
            if(i==n-1) v.push_back(c->data);
            if(c->left) q.push(c->left);
            if(c->right) q.push(c->right);
        }
    }
    return v;
}

int main(){
    vector<int> a={1,2,-1,4,-1,5,6};
    Node* r=build(a);
    auto v=rightView(r);
    for(int x:v) cout<<x<<" ";
}
