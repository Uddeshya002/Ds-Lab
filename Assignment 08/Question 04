#include <iostream>
using namespace std;

struct Node{
    int data;
    Node *left,*right;
    Node(int v):data(v),left(NULL),right(NULL){}
};

bool isBSTUtil(Node* r,long mn,long mx){
    if(!r) return true;
    if(r->data<=mn||r->data>=mx) return false;
    return isBSTUtil(r->left,mn,r->data)&&isBSTUtil(r->right,r->data,mx);
}

bool isBST(Node* r){
    return isBSTUtil(r,LONG_MIN,LONG_MAX);
}

int main(){
    Node* r=new Node(10);
    r->left=new Node(5);
    r->right=new Node(20);
    cout<<(isBST(r)?"YES":"NO");
}
